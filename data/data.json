    "carrierRequirements": [
        {
            "propertyName": "ApplicantGivenName",
            "validator": {
                "type": "notEquals",
                "values": [
                    "",
                    null
                ],
                "conditions": []
            },
            "explanation": "Applicant Given Name is required."
        },
        {
            "propertyName": "ApplicantSurname",
            "validator": {
                "type": "notEquals",
                "values": [
                    "",
                    null
                ],
                "conditions": []
            },
            "explanation": "Applicant Surname is required."
        },
        {
            "propertyName": "ApplicantDOB",
            "validator": {
                "type": "regex",
                "test": "^\\d{4}-\\d{2}-\\d{2}$",
                "conditions": []
            },
            "explanation": "Date of birth is required and must be in YYYY-MM-DD format."
        },
        {
            "propertyName": "InsuranceStatus",
            "validator": {
                "type": "notEquals",
                "values": [
                    "",
                    null
                ],
                "conditions": []
            },
            "explanation": "Client Insurance Status is required."
        },
        {
            "propertyName": "PriorCarrier",
            "validator": {
                "type": "notEquals",
                "values": [
                    "",
                    null
                ],
                "conditions": [
                    {
                        "propertyName": "InsuranceStatus",
                        "validator": {
                            "type": "equals",
                            "values": [
                                "CI"
                            ]
                        }
                    }
                ]
            },
            "explanation": "Prior Carrier is required if Insurance Status is currently insured."
        },
        {
            "propertyName": "UMPD",
            "validator": {
                "type": "equals",
                "values": [
                    null
                ],
                "conditions": [
                    {
                        "propertyName": "Collision",
                        "validator": {
                            "type": "equals",
                            "values": [
                                0,
                                25,
                                50,
                                100,
                                200,
                                250,
                                300
                            ]
                        }
                    }
                ]
            },
            "explanation": "UMPD and Collision may not be selected together."
        },
        {
            "propertyName": "Collision",
            "validator": {
                "type": "equals",
                "values": [
                    null
                ],
                "conditions": [
                    {
                        "propertyName": "UMPD",
                        "validator": {
                            "type": "equals",
                            "values": [
                                3500,
                                5000,
                                7500,
                                10000
                            ]
                        }
                    }
                ]
            },
            "explanation": "Collision and UMPD may not be selected together."
        }
    ],
